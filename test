#!/bin/bash
# romanv automation script

make ocamlc || exit
make ocamlopt || exit

cd testsuite

rm -f _ocamltest/tests/typing-misc/polyvars/dump.txt

touch /tmp/is_test
make one TEST=tests/typing-misc/polyvars.ml
rm /tmp/is_test

echo
cat _ocamltest/tests/typing-misc/polyvars/dump.txt

./clean > /dev/null
